{
  "name": "astappiev/wp-sync-cli",
  "type": "wp-cli-package",
  "description": "A WP-CLI command to sync dev and production WordPress sites.",
  "license": "MIT",
  "authors": [
    {
      "name": "Oleh Astappiev",
      "homepage": "https://github.com/astappiev"
    }
  ],
  "homepage": "https://github.com/astappiev/wp-sync-cli",
  "prefer-stable": true,
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.0",
    "wp-cli/wp-cli": "^2.12"
  },
  "config": {
    "process-timeout": 7200,
    "sort-packages": true
  },
  "extra": {
    "bundled": false,
    "commands": [
      "pull"
    ]
  },
  "autoload": {
    "files": [
      "wp-sync-cli.php"
    ]
  }
}
